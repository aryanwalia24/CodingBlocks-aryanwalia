#include <stdio.h>
int fact(int n)
{
    int f;
    for (f = 1; n > 1; n--)

        f *= n;

    return f;
}
int nCr(int n, int r)
{
    return fact(n) / (fact(n - r) * fact(r));
}

int main()
{
    int n,n1, i, j;
    scanf("%d", &n1);
	n=n1-1;
    for (i = 0; i <= n; i++)
    {
        for (j = 0; j <= i; j++)
        {
            printf("%d\t", nCr(i, j));
        }
        printf("\n");
    }

    return 0;
}
