#include <stdio.h>

#include<stdio.h>
#include<math.h>
int main()
{
    long int n,final1=0,count=0,r;
    scanf("%ld",&n);
	if(n==0)
	{
		printf("5");
	}
	else
	{
    while (n > 0)
    {
        r = n % 10;
		if(r!=0)
		{
		final1=final1 + r*pow(10,count);
		count++;	
		}
		if(r==0)
		{
        final1=final1 + 5*pow(10,count);
		count++;
		}
		if(n<10){break;}
        n = n / 10;
    }
    printf("%ld", final1);  
	}  
	return 0;
}
