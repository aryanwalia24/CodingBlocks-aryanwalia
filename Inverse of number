#include<stdio.h>
int main(void) {
	char c[20];
	scanf("%s",c);
	int count=0,ncount=1;
	for(int i=0;c[i]!='\0';i++){count++;}
	char c1[count];
	for(int i=count-1;i>=0;i--)
	{
		int new1 = (int)c[i] - 48;
		c1[(count-1)-(new1-1)] = (char)(ncount + 48);
		ncount++;
	}
	printf("%s\n",c1);
	return 0;
}
