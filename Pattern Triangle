#include <stdio.h>

int main()
{
    int n ;
	scanf("%d",&n);
    for (int i = 1; i <= n; i++)
    {
        for (int j = n; j > i; j--)
        {
            printf("\t");
        }
        for (int k = i; k < 2 * i; k++)
        {
            printf("%d\t", k);
        }

        for (int k = 2*(i-1); k >=i; k--)
        {
            printf("%d\t", k);
        }
        printf("\n");
    }

    return 0;
}
