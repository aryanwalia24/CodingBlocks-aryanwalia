
#include <stdio.h>

int main()
{
    int n ;
    scanf("%d",&n);
    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; j <= i; j++)
        {
            if (j==1 || j == i)
            {

                printf("%d\t", i);
            }
            else
            {
                printf("0\t");
            }
        }
        printf("\n");
    }

    return 0;
}
