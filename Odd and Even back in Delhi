#include <stdio.h>
int main(void)
{
	int n, n1;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		n1=0;
		int even=0,odd=0;
		scanf("%d", &n1);
		while (n1 > 0)
		{
			int r = n1 % 10;
			if(r%2==0)
			{
				even=even+r;
			}
			else
			{
				odd=odd+r;
			}
			n1 = n1 / 10;
		}
		if(odd%3==0 || even%4==0)
		{
			printf("Yes\n");
		}
		else
		{
			printf("No\n");
		}
	}
	return 0;
}
